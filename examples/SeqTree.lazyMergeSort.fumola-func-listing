func lazyMergeSort(t: SeqTree<X>) : LazyList_<X> {
  do within space `lazyMergeSort{
    `result := thunk {
      reduceUp(
        t,
        null,
        func (symbol: Symbol, element: X) : LazyList<X> {
          ?{symbol=symbol; element; next=nullThunkPointer()}
        },
        func (
          symbol: Symbol,
          xs: LazyList<X>,
          ys: LazyList<X>
        ) : LazyList<X> {
          do within space (`merge - symbol){ merge(xs, ys) }
        },
      )
    }
  }
}
